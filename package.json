{
  "name": "elixir-cloud-components",
  "version": "1.2.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/web-components/web-components.esm.js",
  "homepage": "https://web-components-beta.vercel.app/",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "npm run build:css && stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "build:css": "tailwind build src/global/style.css -o src/global/app.css",
    "deploy": "npm run build && gh-pages -d www"
  },
  "devDependencies": {
    "@stencil/core": "^2.3.0",
    "@stencil/postcss": "^2.0.0",
    "@stencil/store": "^1.4.1",
    "@types/jest": "^26.0.23",
    "@types/puppeteer": "^5.4.3",
    "@types/react": "^17.0.38",
    "autoprefixer": "^10.2.6",
    "gh-pages": "^3.2.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "puppeteer": "^5.5.0",
    "stencil-router-v2": "^0.6.0",
    "tailwindcss": "^2.1.4",
    "workbox-build": "^4.3.1"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "react-tiny-popover": "^7.0.1"
  }
}
